<% if @c.logged_in? && @user %>
<!--
<pre>
<%#= response['status'] %>
<img src="<%#= response['icon_url'] %>" width="24" height="24" align="absmiddle">
<%#= response['email'] %>
<%#= response['counter'] %>
<%#= response['user_id'] %>
